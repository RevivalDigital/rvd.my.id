{
  "crons": [
    {
      "path": "/api/cron",
      "schedule": "0 7 * * *"
    }
  ],
  "rewrites": [
    {
      "source": "/_next/:path*",
      "destination": "https://revival-erp.vercel.app/_next/:path*"
    },
    {
      "source": "/:path*",
      "destination": "https://revival-erp.vercel.app/:path*"
    }
  ]
}
